meta {
  name: Login (Bad Password)
  type: http
  seq: 6
}

post {
  url: {{url}}/User/Login
  body: json
  auth: none
}

body:json {
  {
      "username": "{{login-username}}",
      "password": "{{test-password}}-INVALID"
  }
}

assert {
  res.status: eq 200
  res.body.response: isNull {{login-username}}
  res.body.errors[0]: eq Invalid username or password.
}
